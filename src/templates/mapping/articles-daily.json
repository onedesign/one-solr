[
  {% for entry in craft.entries.sectionId(sectionId).id(id).limit(limit).offset(offset) %}
  {
    "id": "{{ entry.id }}",
    "section_id_i": {{ entry.sectionId | raw }},
  "entry_id_i": {{ entry.id }},
  "article_type_i": 4,
  "title": {{ entry.title | json_encode() | raw }},
  "url": {{ entry.url | json_encode() | raw }},
  "post_date_dt": "{{ entry.postDate | date('Y-m-d\\TH:i:s\\Z') }}",
  "content": "content",
  "keywords": "keyword",
  "tags_ss": [
  "tag1", "tag2"
],
  "categories_ss": [
  "category1", "category2"
]

}{% if not loop.last %},{% endif %}
  {% endfor %}
]